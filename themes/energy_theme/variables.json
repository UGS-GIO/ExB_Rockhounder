      },
      "danger": {
        "100": "#3f0a11",
        "200": "#6b111d",
        "300": "#971829",
        "400": "#c31f35",
        "500": "#df364d",
        "600": "#e55e70",
        "700": "#ec8593",
        "800": "#f2adb6",
