      },
      "dark": {
        "100": "#808080",
        "200": "#8f8f8f",
        "300": "#9f9f9f",
        "400": "#afafaf",
        "500": "#bfbfbf",
        "600": "#cfcfcf",
        "700": "#dfdfdf",
        "800": "#efefef",
        "900": "#ffffff"
      }
    }
  },
  "colorationVariants": {
    "default": {
      "header": {
        "bg": "var(--primary-300)",
        "color": "white"
      },
      "body": {
        "bg": "var(--light-100)",
        "color": "var(--black)"
      },
      "footer": {
        "bg": "var(--light-100)",
        "color": "var(--black)"
      }
    }
  },
  "surfaces": {
    "1": {
      "bg": "var(--light-400)",
      "border": {
        "color": "var(--light-600)"
      }
    },
    "2": {
      "bg": "var(--light-300)",
      "border": {
        "color": "var(--light-600)"
      }
    }
  },
  "link": {
    "color": "var(--primary-600)",
    "hover": {
      "color": "var(--primary-700)"
    }
  },
  "components": {
    "button": {
      "variants": {
        "default": {
          "default": {
            "color": "var(--black)",
            "bg": "var(--light-500)",
            "border": {
              "color": "var(--light-500)"
            }
          },
          "hover": {
            "bg": "var(--light-600)"
          },
          "disabled": {
            "color": "var(--dark-200)",
            "bg": "var(--light-400)",
            "border": {
              "color": "var(--light-600)"
            }
          }
        },
        "primary": {
          "default": {
            "color": "var(--black)"
          },
          "disabled": {
            "color": "var(--dark-200)",
            "bg": "var(--light-400)",
            "border": {
              "color": "var(--light-400)"
            }
          }
        },
        "secondary": {
          "disabled": {
            "color": "var(--dark-200)",
            "bg": "var(--light-400)",
            "border": {
              "color": "var(--light-600)"
            }
          }
        },
        "tertiary": {
          "disabled": {
            "color": "var(--dark-200)",
            "border": {
              "color": "transparent",
              "width": "1px"
            }
          }
        },
        "danger": {
          "default": {
            "color": "var(--black)"
          },
          "hover": {
            "color": "var(--white)"
          },
          "disabled": {
            "color": "var(--dark-200)",
            "bg": "var(--light-400)",
            "border": {
              "color": "var(--light-400)"
            }
          }
        },
        "link": {
          "default": {
            "color": "var(--primary-600)"
          },
          "hover": {
            "color": "var(--primary-700)"
          },
          "active": {
            "color": "var(--primary-700)"
          },
          "disabled": {
            "color": "var(--dark-200)"
          }
        }
      }
    },
    "progress": {
      "bg": "var(--light-800)"
    },
    "modal": {
      "backdrop": {
        "bg": "var(--white)"
      }
    }
  }
}